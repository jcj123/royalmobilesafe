<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:jcj="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        style="@style/TitleTheme"
        android:text="设置中心" />

    <royal.com.qs.jcj.view.SettingView
        android:id="@+id/stv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        jcj:desc_off="自动更新已关闭"
        jcj:desc_on="自动更新已开启"
        jcj:setting_title="自动更新设置" />

    <royal.com.qs.jcj.view.SettingView
        android:id="@+id/stv_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        jcj:desc_off="归属地显示已关闭"
        jcj:desc_on="归属地显示已开启"
        jcj:setting_title="电话归属地显示设置" />

    <royal.com.qs.jcj.view.SettingDialog
        android:id="@+id/view_sd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <royal.com.qs.jcj.view.SettingDialog
        android:id="@+id/view_position_sd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
